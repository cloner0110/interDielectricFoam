FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      gamma;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    domainWall
    {
        type            zeroGradient;
    }
    floor
    {
        type            fixedValue;
        value           uniform 3.16227766016838;
    }
    top
    {
        type            fixedValue;
        value           uniform 0;
    }
    axis
    {
        type            empty;
    }
}

